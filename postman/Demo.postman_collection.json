			"name": "Create Closing",
			"item": [
				{
					"name": "Step 1 create a closing record",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const jsonData = pm.response.json();",
									"pm.collectionVariables.set(\"ClosingUUID\", jsonData.closing_uuid);"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// today format yyyy-mm-dd",
									"var d1 = new Date(new Date().getFullYear(), 0, 1).toISOString().split('T')[0];",
									"var today = new Date()",
									"var ts = (today.getMonth() + 1).toString().padStart(2, '0') + today.getDate().toString().padStart(2, '0') +today.getHours().toString().padStart(2, '0') + today.getMinutes().toString().padStart(2, '0')",
									"pm.collectionVariables.set(\"appointment_earliest_at\", today.toISOString().split('T')[0]);",
									"pm.collectionVariables.set(\"appointment_date\", today.toISOString().split('T')[0]);",
									"",
									"today.setDate(today.getDate() + 7)",
									"pm.collectionVariables.set(\"appointment_latest_at\", today.toISOString().split('T')[0]);",
									"",
									"pm.collectionVariables.set(\"borrower_1_last_name\", \"Test\" + ts)",
									"pm.collectionVariables.set(\"borrower_1_email\", \"borrower1.Test\" + ts+\"@snapdocs.com\")",
									"pm.collectionVariables.set(\"borrower_2_last_name\", \"Test\" + ts)",
									"pm.collectionVariables.set(\"borrower_2_email\", \"borrower2.Test\" + ts+\"@snapdocs.com\")",
									"pm.collectionVariables.set(\"file_number\", \"file\"+ts)",
									"pm.collectionVariables.set(\"external_value\", \"loan\" + ts)",
									"pm.collectionVariables.set(\"timestamp\", ts)"
								],
								"type": "text/javascript"
							}
						}
					],
