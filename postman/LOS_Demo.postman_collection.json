		{
			"name": "create closing",
			"item": [
				{
					"name": "Step 1 create a closing record",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const jsonData = pm.response.json();",
									"pm.collectionVariables.set(\"ClosingUUID\", jsonData.closing_uuid);"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// today format yyyy-mm-dd",
									"var d1 = new Date(new Date().getFullYear(), 0, 1).toISOString().split('T')[0];",
									"",
									"// the last day of the year",
									"var d365 = new Date(new Date().getFullYear(), 11, 31).toISOString().split('T')[0];",
									"var today = new Date().toISOString().split('T')[0]",
									"var ts = (new Date().getMonth() + 1).toString().padStart(2, '0') + new Date().getDate().toString().padStart(2, '0') + new Date().getHours().toString().padStart(2, '0') + new Date().getMinutes().toString().padStart(2, '0')",
									"pm.collectionVariables.set(\"appointment_earliest_at\", d1);",
									"pm.collectionVariables.set(\"appointment_latest_at\", d365);",
									"pm.collectionVariables.set(\"appointment_date\", today);",
									"",
									"pm.collectionVariables.set(\"borrower_1_last_name\", \"Test\" + ts)",
									"",
									"pm.collectionVariables.set(\"borrower_2_last_name\", \"Test\" + ts)",
									"pm.collectionVariables.set(\"file_number\", \"file\"+ts)",
									"pm.collectionVariables.set(\"loan_guid\", \"loan\" + ts)",
									"pm.collectionVariables.set(\"timestamp\", ts)"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"auth": {
							"type": "bearer",
							"bearer": [
								{
									"key": "token",
									"value": "{{ApiToken}}",
									"type": "string"
								}
							]
						},
						"method": "POST",
						"header": [
							{
								"key": "Content-type",
								"value": "application/json",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n     \"signing_method\": \"hybrid\",\n     \"source\": \"encompass\",\n     \"reference_id\": \"{{loan_guid}}\",\n     \"file_number\": \"{{file_number}}\",\n     \"funding_team_email\": \"isaac+closings@snapdocs.com\",\n     \"property_street_address\": \"1010 Van Ness St\",\n     \"property_city\": \"Austin\",\n     \"property_state\": \"Texas\",\n     \"property_zip\": \"78701\",\n     \"appointment_earliest_at\": \"{{appointment_earliest_at}}\",\n    \"appointment_latest_at\": \"{{appointment_latest_at}}\",\n    \"appointment_date\": \"{{appointment_date}}\",\n    \"appointment_time\": \"11:45 pm\",\n     \"settlement_office_email\": \"settlement@settlement.com\",\n     \"settlement_office_name\": \"Settlement Office\",\n     \"borrowers\": [\n          {\n               \"first_name\": \"{{borrower_1_first_name}}\",\n               \"last_name\": \"{{borrower_1_last_name}}\",\n               \"email\": \"{{borrower_1_email}}\",\n               \"phone\": \"{{borrower_1_phone}}\"\n          }\n     ],\n     \"closing_users\": [\n     ],\n     \"settlement_agents\": [\n          {\n               \"first_name\": \"Settlement\",\n               \"last_name\": \"Agent\",\n               \"email\": \"sa@settlement.com\"\n          }\n     ],\n     \"external_identifiers\": [\n          {\n               \"external_system\": \"{{external_system}}\",\n               \"external_type\": \"{{external_type}}\",\n               \"value\": \"{{loan_guid}}\"            \n          }\n     ]\n}"
						},
						"url": {
							"raw": "{{BaseUrl}}/api/v1/closings",
							"host": [
								"{{BaseUrl}}"
							],
							"path": [
								"api",
								"v1",
								"closings"
							]
						},
						"description": "use the access token from prev call"
					},
					"response": []
				},
